{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "github": {
    "enabled": true,
    "silent": true
  },
  "rewrites": [
    {
      "source": "/lifeos",
      "destination": "/lifeos"
    },
    {
      "source": "/lifeos/:path*",
      "destination": "/lifeos/:path*"
    }
  ]
}

